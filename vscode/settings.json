        "terminal.ansiYellow": "#c7c400",
        "terminal.ansiBlue": "#504dc7",
        "terminal.ansiMagenta": "#bf55b1",
        "terminal.ansiCyan": "#00c5c7",
        "terminal.ansiWhite": "#c7c7c7",
        "terminal.ansiBrightBlack": "#676767",
        "terminal.ansiBrightRed": "#dc7974",
        "terminal.ansiBrightGreen": "#57e690",
        "terminal.ansiBrightYellow": "#ece100",
        "terminal.ansiBrightBlue": "#a6aaf1",
        "terminal.ansiBrightMagenta": "#e07de0",
